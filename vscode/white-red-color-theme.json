  "semanticTokenColors": {
    "comment": {
      "foreground": "#777777",
      "italic": true
    },
    "keyword": {
      "foreground": "#3A1512",
      "bold": true
    },
    "string": {
      "foreground": "#6B221C"
    },
    "number": {
      "foreground": "#8A2922"
    },
    "regexp": {
      "foreground": "#6B221C"
    },
    "operator": {
      "foreground": "#4D1B16"
    },
    "function": {
      "foreground": "#AF2F28"
    },
    "method": {
      "foreground": "#AF2F28"
    },
    "parameter": {
      "foreground": "#8A2922"
    },
    "property": {
      "foreground": "#6B221C"
    },
    "variable": {
      "foreground": "#4D1B16"
    },
    "type": {
      "foreground": "#3A1512"
    },
    "class": {
      "foreground": "#3A1512"
    },
    "enum": {
      "foreground": "#4D1B16"
    },
    "interface": {
      "foreground": "#3A1512"
    },
    "namespace": {
      "foreground": "#3A1512"
    }
  },
  "tokenColors": [
    {
      "name": "Comments",
      "scope": [
        "comment",
        "punctuation.definition.comment"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#777777"
      }
    },
    {
      "name": "Punctuation & Brackets",
      "scope": [
        "punctuation",
        "meta.brace",
        "meta.delimiter",
        "punctuation.definition.block",
        "punctuation.separator",
        "punctuation.definition.dictionary",
        "punctuation.definition.array"
      ],
      "settings": {
        "foreground": "#8A2922"
      }
    },
    {
      "name": "Keywords",
      "scope": [
        "keyword",
        "storage.type",
        "storage.modifier"
      ],
      "settings": {
        "foreground": "#3A1512",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Strings",
      "scope": [
        "string"
      ],
      "settings": {
        "foreground": "#6B221C"
      }
    },
    {
      "name": "Numbers",
      "scope": [
        "constant.numeric",
        "constant.language"
      ],
      "settings": {
        "foreground": "#8A2922"
      }
    },
    {
      "name": "Functions",
      "scope": [
        "entity.name.function",
        "support.function"
      ],
      "settings": {
        "foreground": "#AF2F28"
      }
    },
    {
      "name": "Types",
      "scope": [
        "entity.name.type",
        "support.type",
        "support.class"
      ],
      "settings": {
        "foreground": "#3A1512"
      }
    },
    {
      "name": "Variables",
      "scope": [
        "variable",
        "variable.parameter"
      ],
      "settings": {
        "foreground": "#4D1B16"
      }
    },
    {
      "name": "Properties",
      "scope": [
        "variable.other.property",
        "support.variable.property",
        "meta.object-literal.key"
      ],
      "settings": {
        "foreground": "#6B221C"
      }
    },
    {
      "name": "Operators",
      "scope": [
        "keyword.operator"
      ],
      "settings": {
        "foreground": "#4D1B16"
      }
    },
    {
      "name": "Constants",
      "scope": [
        "constant.other",
        "constant.language.boolean"
      ],
      "settings": {
        "foreground": "#8A2922"
      }
    },
    {
      "name": "Classes",
      "scope": [
        "entity.name.class",
        "entity.other.inherited-class"
      ],
      "settings": {
        "foreground": "#3A1512"
      }
    },
    {
      "name": "Tags (HTML/XML)",
      "scope": [
        "entity.name.tag"
      ],
      "settings": {
        "foreground": "#AF2F28"
      }
    },
    {
      "name": "Attributes",
      "scope": [
        "entity.other.attribute-name"
      ],
      "settings": {
        "foreground": "#6B221C"
      }
    },
    {
      "name": "JSON Keys",
      "scope": [
        "support.type.property-name.json"
      ],
      "settings": {
        "foreground": "#474747"
      }
    },
    {
      "name": "JSON String Values",
      "scope": [
        "string.quoted.double.json"
      ],
      "settings": {
        "foreground": "#6B221C"
      }
    },
    {
      "name": "JSON Punctuation",
      "scope": [
        "punctuation.definition.dictionary.begin.json",
        "punctuation.definition.dictionary.end.json",
        "punctuation.definition.array.begin.json",
        "punctuation.definition.array.end.json",
        "punctuation.separator.dictionary.pair.json",
        "punctuation.separator.dictionary.key-value.json",
        "punctuation.separator.array.json"
      ],
      "settings": {
        "foreground": "#5E5E5E"
      }
    },
    {
      "name": "JSON Numbers",
      "scope": [
        "constant.numeric.json"
      ],
      "settings": {
        "foreground": "#8A2922"
      }
    },
    {
      "name": "JSON Boolean/Null",
      "scope": [
        "constant.language.json"
      ],
      "settings": {
        "foreground": "#AF2F28"
      }
    }
  ]
}